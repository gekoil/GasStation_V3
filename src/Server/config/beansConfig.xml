<?xml version="1.0" encoding="utf-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="gasStation" class="BL.GasStation"/>
    <bean id="car" class="BL.Car"/>
    <bean id="cleaning" class="BL.CleaningService"/>
    <bean id="gasSupplier" class="BL.GasSupplier"/>
    <bean id="mainFuelPool" class="BL.MainFuelPool"/>
    <bean id="manualClean" class="BL.ManualClean"/>
    <bean id="pump" class="BL.Pump"/>
    <bean id="statistics" class="BL.Statistics"/>
    <bean id="iDAL" class="DAL.ConnectorDAL"/>
    <!--<bean id="persistenceDAL" class="DAL.PersistenceDAL"/>-->
</beans>